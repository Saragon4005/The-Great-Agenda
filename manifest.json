{
  "manifest_version": 2,

  "name": "The Agenda",
  "description": "Quick and easy time management",
  "version": "0.9.0",
  "icons": { "16": "AgendaUnstable.png",
             "48": "AgendaUnstable_48.png",
            "128": "AgendaUnstable_128.png" },
  "browser_action": {
    "default_icon": "AgendaUnstable.png",
    "default_popup": "mainWindow.html"
  },

  "permissions": ["activeTab", "tabs"]
}
